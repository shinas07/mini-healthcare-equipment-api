# App
APP_NAME=Mini Healthcare Equipment API
APP_ENV=development
DEBUG=false
API_PREFIX=

# Option A: direct SQLAlchemy async URL (recommended)
# Example PostgreSQL:
# DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/healthcare_db
# Example SQLite:
# DATABASE_URL=sqlite+aiosqlite:///./mini_healthcare.db
DATABASE_URL=

# Option B: build DB URL from parts (used when DATABASE_URL is empty)
DBUSER=postgres
DBPASS=postgres
DBHOST=localhost
DBNAME=healthcare_db
DBPORT=5432
